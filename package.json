{
  "name": "ai-governance",
  "version": "0.1.0",
  "description": "Framework and orchestration for AI governance of software projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate": "node-pg-migrate",
    "typecheck": "tsc --noEmit",
    "validate:github": "tsx scripts/validate-github-app.ts"
  },
  "keywords": [
    "ai",
    "governance",
    "agent",
    "claude",
    "mcp"
  ],
  "author": "AI Governance Contributors",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.37",
    "@anthropic-ai/sdk": "^0.74.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.985.0",
    "@aws-sdk/client-s3": "^3.985.0",
    "@aws-sdk/client-secrets-manager": "^3.985.0",
    "@fastify/static": "^9.0.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-logs-otlp-grpc": "^0.211.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.211.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.211.0",
    "@opentelemetry/sdk-metrics": "^2.5.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "dotenv": "^17.2.4",
    "fastify": "^5.7.4",
    "ioredis": "^5.3.2",
    "langfuse": "^3.38.6",
    "openai": "^6.18.0",
    "pg": "^8.18.0",
    "pgvector": "^0.2.0",
    "probot": "^13.1.0",
    "yaml": "^2.3.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.2",
    "@types/pg": "^8.11.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^10.0.0",
    "jest": "^30.2.0",
    "node-pg-migrate": "^7.0.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
